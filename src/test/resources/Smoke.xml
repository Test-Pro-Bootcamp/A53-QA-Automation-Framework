<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >

<suite name="Smoke">
    <parameter name="BaseURL" value="https://qa.koel.app"></parameter>
    <test name="Smoke Test" preserve-order="false">
        <classes>
            <class name="LoginTest">
                <methods>
                    <include name="loginWithCorrectCredentials"/>
                    <include name="loginInvalidEmailValidPassword"/>
                </methods>
            </class>
        </classes>
    </test>
</suite>